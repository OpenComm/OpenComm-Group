\par }{\rtlch\fcs1 \af2 \ltrch\fcs0 \f2\ulc17\insrsid799343 /**
\par  * Controller called from LoginView. Takes care of login functionality.
\par  * It connects to the server and returns true if the login is a success.
\par  * If Login is successful it calls DashboardView.
\par  *
\par  * Issues [TODO]
\par  * - No login incorrect message on screen
\par  * - ProgressDialog is visible even if login is incorrect
\par  * - For any other issues search for string "TODO"
\par  *
\par  * @author rahularora[hcisec], vinaymaloo[ui]
\par  * */
\par 
\par package edu.cornell.opencomm.controller;
\par 
\par import java.util.concurrent.locks.ReentrantLock;
\par 
\par import org.jivesoftware.smack.XMPPException;
\par 
\par import android.content.Intent;
\par import android.os.AsyncTask;
\par import android.util.Log;
\par import android.view.View;
\par import android.widget.EditText;
\par import edu.cornell.opencomm.Values;
\par import edu.cornell.opencomm.network.Network;
\par import edu.cornell.opencomm.network.NetworkService;
\par import edu.cornell.opencomm.view.DashboardView;
\par import edu.cornell.opencomm.view.LoginView;
\par import edu.cornell.opencomm.view.NotificationView;
\par 
\par public class LoginController \{
\par \tab private LoginView loginView;
\par 
\par \tab // Debugging
\par \tab private static final boolean D = Values.D;
\par 
\par \tab // Logs
\par \tab private static final String LOG_TAG = "LoginController";
\par 
\par \tab // Check successful login
\par \tab private boolean islogin;
\par 
\par \tab private enum ReturnState\{SUCEEDED, COULDNT_CONNECT, WRONG_PASSWORD, ALREADY_CLICKED\};
\par 
\par \tab // Username and password strings
\par \tab private String username;
\par \tab private String password;
\par 
\par \tab private ReentrantLock loginLock = new ReentrantLock();
\par 
\par \tab // Instance of XMPP connection
\par \tab public static NetworkService xmppService;
\par 
\par \tab public LoginController(LoginView loginView) \{
\par \tab \tab this.loginView = loginView;
\par \tab \}
\par 
\par \tab public void handleLoginButtonClick(final EditText usernameEdit, final EditText passwordEdit) \{
\par \tab \tab new LoginTask().execute(usernameEdit.getText().toString(), passwordEdit.getText().toString());
\par 
\par \tab \}
\par 
\par \tab private class LoginTask extends AsyncTask<String, Void, ReturnState> \{
\par 
\par 
\par \tab \tab @Override
\par \tab \tab protected void onPreExecute() \{
\par \tab \tab \tab loginView.getLoginOverlay().setVisibility(View.VISIBLE);
\par \tab \tab \}
\par 
\par \tab \tab @Override
\par \tab \tab protected ReturnState doInBackground(String... strings) \{
\par \tab \tab \tab if(loginLock.isLocked()) return ReturnState.ALREADY_CLICKED;
\par \tab \tab \tab loginLock.lock();
\par \tab \tab \tab try\{
\par \tab \tab \tab \tab if (D) \{
\par \tab \tab \tab \tab \tab Log.d(LOG_TAG, "Android app is attempting to connect to the server");
\par \tab \tab \tab \tab \tab username = Network.DEBUG_USERNAME;
\par \tab \tab \tab \tab \tab password = Network.DEBUG_PASSWORD;
\par \tab \tab \tab \tab \}
\par \tab \tab \tab \tab else\{
\par \tab \tab \tab \tab \tab //username = strings[0];
\par \tab \tab \tab \tab \tab 
\par \tab \tab \tab \tab \tab String[] temp = strings[0].split("@");
\par \tab \tab \tab \tab \tab try\{
\par \tab \tab \tab \tab \tab \tab username = temp[0]+temp[1];
\par \tab \tab \tab \tab \tab \}
\par \tab \tab \tab \tab \tab catch (ArrayIndexOutOfBoundsException e) \{
\par \tab \tab \tab \tab \tab \tab username = temp[0];
\par \tab \tab \tab \tab \tab \}
\par \tab \tab \tab \tab \tab password = strings[1];
\par \tab \tab \tab \tab \}
\par 
\par \tab \tab \tab \tab if (D) Log.d(LOG_TAG, "Got Here1");
\par 
\par \tab \tab \tab \tab xmppService = new NetworkService(Network.DEFAULT_HOST, Network.DEFAULT_PORT);
\par \tab \tab \tab \tab if (xmppService.isConnected())\{
\par \tab \tab \tab \tab \tab if (D) \{
\par \tab \tab \tab \tab \tab \tab Log.d(LOG_TAG, xmppService.toString());
\par \tab \tab \tab \tab \tab \tab Log.d(LOG_TAG, "XMPP Connection established");
\par 
\par \tab \tab \tab \tab \tab \}
\par \tab \tab \tab \tab \}
\par \tab \tab \tab \tab else \{
\par \tab \tab \tab \tab \tab return ReturnState.COULDNT_CONNECT;
\par \tab \tab \tab \tab \}
\par 
\par \tab \tab \tab \tab /** Check whether the login is successful or not
\par \tab \tab \tab \tab  * In case it is, start DashboardView using Intent else, [TODO]
\par                  / @author: rahularora, vinaymaloo **/
\par \tab \tab \tab \tab if (D) Log.d(LOG_TAG, "Got Here2");
\par \tab \tab \tab \tab try \{
